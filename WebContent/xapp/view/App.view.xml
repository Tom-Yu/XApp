<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" controllerName="xapp.controller.App" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="功能列表 " class="sapUiResponsiveContentPadding">
		<content>
			<sap.ui.layout:Grid xmlns:sap.ui.layout="sap.ui.layout"
				id="__grid0" content="{/Products}" containerQuery="true">
				<sap.ui.layout:content>
					<ObjectListItem type="Navigation" title="{UnitsOnOrder}"
						number="{UnitPrice}" numberUnit="{QuantityPerUnit}" intro="{ProductName}"
						icon="sap-icon://laptop" id="__item2" activeIcon="sap-icon://accept"
						selected="true" showMarkers="true" markLocked="true" markFlagged="true" />
				</sap.ui.layout:content>
			</sap.ui.layout:Grid>
		</content>
		<footer>
			<Toolbar class="sapContrast sapContrastPlus" ariaLabelledBy="__xmlview0--__item7-label"
				width="100%" id="__toolbar2">
				<content>
					<Label text="@CopyRight 2016-2017" width="100%" id="__label1"
						textAlign="Center" />
				</content>
			</Toolbar>
		</footer>
		<headerContent>
			<Button width="60px" id="openMenu" icon="sap-icon://menu"
				press="handlePressOpenMenu" />
		</headerContent>
	</Page>
</core:View>